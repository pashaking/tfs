  <DataSets>
    <DataSet Name="SprintList">
      <Query>
        <DataSourceName>tfs_warehouse</DataSourceName>
        <CommandText>SELECT *,(SELECT IterationSK FROM DimIteration WHERE StartDate &lt;= GETDATE() AND FinishDate &gt;= GETDATE()) CurrentIteration
FROM DimIteration WHERE StartDate IS NOT NULL AND StartDate &gt; DATEADD(MONTH,-6,GETDATE()) 
ORDER BY StartDate desc</CommandText>
      </Query>
      <Fields>
