    <DataSet Name="SprintItemsList">
      <Query>
        <DataSourceName>tfs_warehouse</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IterationSK">
            <Value>=Parameters!SprintName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ResourceList">
            <Value>=Parameters!Resource.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ActivityList">
            <Value>=Parameters!Activity.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @StartDate DATETIME, @EndDate DATETIME
SELECT @StartDate= StartDate, @EndDate = (CASE WHEN DATEPART(DW,FinishDate) = 6 /*FRIDAY*/ THEN DATEADD(DAY,2,FinishDate) ELSE FinishDate END)
FROM DimIteration  WHERE IterationSK = @IterationSK

SELECT dp.Name, cwiv.System_Id ItemNo,cwiv.System_WorkItemType ItemType,cwiv.System_Title Title,
cwiv.System_State [State],cwiv.Microsoft_VSTS_Common_Activity Activity,cwiv.iterationPath,
SUM(fwih.Microsoft_VSTS_Scheduling_OriginalEstimate) OriginalEstimate,
SUM(fwih.Microsoft_VSTS_Scheduling_CompletedWork) CompletedWork,
SUM(fwih.Microsoft_VSTS_Scheduling_RemainingWork) RemainingWork,
'http://tfs2013-vm:8080/tfs/AKACollection/Raisin/Team B/_workitems#id=' + CAST(cwiv.System_Id as varchar(MAX)) + '&amp;_a=edit' URL
FROM FactWorkItemHistory fwih 
JOIN DimWorkItem dwi ON dwi.WorkItemSK = fwih.WorkItemSK 
JOIN DimPerson dp ON dp.PersonSK = dwi.System_AssignedTo__PersonSK
JOIN CurrentWorkItemView cwiv ON cwiv.System_Id = dwi.System_Id
WHERE 
fwih.DateSK&gt;=@StartDate
AND fwih.DateSK&lt;=@EndDate
AND dp.Name IN (@ResourceList)
AND cwiv.Microsoft_VSTS_Common_Activity IN (@ActivityList)
AND isnull(fwih.Microsoft_VSTS_Scheduling_CompletedWork,0) &lt;&gt;0
group by dp.Name,cwiv.System_Id,cwiv.System_WorkItemType,cwiv.System_Title,cwiv.System_State,cwiv.Microsoft_VSTS_Common_Activity,cwiv.iterationPath
having sum(fwih.Microsoft_VSTS_Scheduling_CompletedWork) &lt;&gt; 0
ORDER BY dp.Name, ItemNo</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemNo">
          <DataField>ItemNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ItemType">
          <DataField>ItemType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Activity">
          <DataField>Activity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="iterationPath">
          <DataField>iterationPath</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OriginalEstimate">
          <DataField>OriginalEstimate</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="CompletedWork">
          <DataField>CompletedWork</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="RemainingWork">
          <DataField>RemainingWork</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="URL">
          <DataField>URL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ActivityList">
      <Query>
        <DataSourceName>tfs_warehouse</DataSourceName>
        <CommandText>select distinct Microsoft_VSTS_Common_Activity ActivityName from CurrentWorkItemView WHERE Microsoft_VSTS_Common_Activity IS NOT NULL AND ISNUMERIC(Microsoft_VSTS_Common_Activity)=0
Order by 1</CommandText>
      </Query>
      <Fields>
        <Field Name="ActivityName">
          <DataField>ActivityName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ResourceList">
      <Query>
        <DataSourceName>tfs_warehouse</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IterationSK">
            <Value>=Parameters!SprintName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @StartDate DATETIME, @EndDate DATETIME
SELECT @StartDate= StartDate, @EndDate = (CASE WHEN DATEPART(DW,FinishDate) = 6 /*FRIDAY*/ THEN DATEADD(DAY,2,FinishDate) ELSE FinishDate END)
FROM DimIteration  WHERE IterationSK = @IterationSK

SELECT DISTINCT dp.Name
FROM FactWorkItemHistory fwih 
JOIN DimWorkItem dwi ON dwi.WorkItemSK = fwih.WorkItemSK 
JOIN DimPerson dp ON dp.PersonSK = dwi.System_AssignedTo__PersonSK
JOIN CurrentWorkItemView cwiv ON cwiv.System_Id = dwi.System_Id
WHERE 
fwih.DateSK&gt;=@StartDate
AND fwih.DateSK&lt;=@EndDate
AND isnull(fwih.Microsoft_VSTS_Scheduling_CompletedWork,0) &lt;&gt;0
group by dp.Name,cwiv.System_Id,cwiv.System_WorkItemType,cwiv.System_Title,cwiv.System_State,cwiv.Microsoft_VSTS_Common_Activity,cwiv.iterationPath
having sum(fwih.Microsoft_VSTS_Scheduling_CompletedWork) &lt;&gt; 0
ORDER BY dp.Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
